<%- include("partials/header") -%>


<h1>Home</h1>
<p><%= homeStartingContent%></p>

<div>
  <% posts.forEach (function(post) { %>
  <h2><%= post.title %></h2>
  <p><%= post.content.substring(0, 100) + "..." %>
